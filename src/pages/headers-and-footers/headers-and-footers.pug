extends ../../templates/ui-kit/ui-kit

include ../../components/header/header
include ../../components/footer/footer

block var
  - const withThemeGray = true;

block title
  | Headers and footers

block main
  -
    const {
      headers, 
      footers
    } = require('./data.json')

  .headers-and-footers
    +header(headers.hasButtons)
    +header(headers.hasNames)
    +footer(footers.default)
    .headers-and-footers__mobile-footer
      +footer(footers.mobile)
